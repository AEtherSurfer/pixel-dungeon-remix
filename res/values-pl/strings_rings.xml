  <!--STRINGS OF RingOfAccuracy CLASS-->
  <string name="RingOfAccuracy_Name">Pierscien Dokladnosci</string>
  <string name="RingOfAccuracy_Info">Ten pierścień zwiększa szansę trafienia przeciwnika</string>
  <!--STRINGS OF RingOfDetection CLASS-->
  <string name="RingOfDetection_Name">Pierscien Cieni</string>
  <string name="RingOfDetection_Info">Noszenie ten pierścień pozwoliużytkownik dostrzec ukryte tajemnice - pułapek i sekretne drzwi,. Natomiast zniszczono o conajmniej -1 zmuli Ciebie i staniesz sie slaby!</string>
  <!--STRINGS OF RingOfElements CLASS-->
  <string name="RingOfElements_Name">Pierscien Zywiolow</string>
  <string name="RingOfElements_Info">Ten pierscien pozwoli Ci sie uotpornic od roznych zywiołow i zmniesza czas trwania neatwnych  gazow i cieczy</string>
  <!--STRINGS OF RingOfEvasion CLASS-->
  <string name="RingOfEvasion_Name">Pierscien Unikow</string>
  <string name="RingOfEvasion_Info">Pierscien Pozwoli CI a czestrze unikanie atakow</string>
  <!--STRINGS OF RingOfHaggler CLASS-->
  <string name="RingOfHaggler_Name">Pierścień Hanggiera !</string>
  <string name="RingOfHaggler_Info">Pierscien nie ma zadych efektow magicznych, ale prawdopodobnie pozwoli ci ukrasc cos ze sklepu</string>
  <!--STRINGS OF RingOfHaste CLASS-->
  <string name="RingOfHaste_Name">Pierscien Predkosci</string>
  <string name="RingOfHaste_Info">Pierscien pozwala na szybsze poruszanie sie.</string>
  <!--STRINGS OF RingOfHerbalism CLASS-->
  <string name="RingOfHerbalism_Name">Leczniczy Pierscien Bogow</string>
  <string name="RingOfHerbalism_Info">Zwieksza szanse na zebranie pozytywnego ziola oraz rosy z Trawy.</string>
  <!--STRINGS OF RingOfMending CLASS-->
  <string name="RingOfMending_Name">Pierscien Odnowy Biologicznej</string>
  <string name="RingOfMending_Info">Pozwala zregenerowac Cialo do Maksimum. </string>
  <!--STRINGS OF RingOfPower CLASS-->
  <string name="RingOfPower_Name">Piersien Sily</string>
  <string name="RingOfPower_Info">Zwieksza SIle</string>
  <!--STRINGS OF RingOfSatiety CLASS-->
  <string name="RingOfSatiety_Name">Pierscien Glodu</string>
  <string name="RingOfSatiety_Info">Majac go na palcu jestes mniej glodny i rzadziej!</string>
  <!--STRINGS OF RingOfShadows CLASS-->
  <string name="RingOfShadows_Name">Pierscien Cieni</string>
  <string name="RingOfShadows_Info">Wrogowie nie zwracaja na Ciebie uwagi. Gdy tylko pierscien osiagnie -1 wrogowie dostrzega cie chocby w piekle!</string>
  <!--STRINGS OF RingOfThorns CLASS-->
  <string name="RingOfThorns_Name">Pierscien Ciernii</string>
  <string name="RingOfThorns_Info">Gdy nosisz go, ataki wroga po czesci odbijaja sie i trafiaja go z rownie poterzna sila co Ciebie!!</string>
  <!--STRINGS OF RingOfStoneWalking CLASS-->
  <string name="RingOfStoneWalking_Name">Krwawy Pierscien Eda</string>
  <string name="RingOfStoneWalking_Info">Posiada Krew Zamiast Klejnotu</string>
  <string name="RingOfStoneWalking_ImmuredInStone">uwieziony w kamieniu. Na zawsze!</string>
